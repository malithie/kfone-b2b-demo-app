{
  "extends": [
    "next/core-web-vitals",
    "./.eslintrc.js"
  ],
  "rules": {
    "max-len": [
      "error",
      {
        "code": 120
      }
    ],
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 1,
        "maxEOF": 1
      }
    ],
    "eol-last": [
      "error",
      "always"
    ]
  }
}
