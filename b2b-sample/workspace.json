{
  "version": 2,
  "projects": {
    "business-app": {
      "root": "apps/business-app",
      "sourceRoot": "apps/business-app",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "defaultConfiguration": "production",
          "options": {
            "root": "apps/business-app",
            "outputPath": "dist/apps/business-app"
          },
          "configurations": {
            "development": {
              "outputPath": "apps/business-app"
            },
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "defaultConfiguration": "development",
          "options": {
            "buildTarget": "business-app:build",
            "dev": true,
            "port": 3000
          },
          "configurations": {
            "development": {
              "buildTarget": "business-app:build:development",
              "dev": true
            },
            "production": {
              "buildTarget": "business-app:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "business-app:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "{workspaceRoot}/coverage/{projectRoot}"
          ],
          "options": {
            "jestConfig": "apps/business-app/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/business-app/**/*.{ts,tsx,js,jsx}"
            ]
          }
        }
      },
      "tags": []
    },
    "business-app-ui-assets": {
      "root": "libs/business-app/ui-assets",
      "sourceRoot": "libs/business-app/ui-assets/src",
      "projectType": "ui-library"
    },
    "shared-ui-assets": {
      "root": "libs/shared/ui-assets",
      "sourceRoot": "libs/shared/ui-assets/src",
      "projectType": "ui-library"
    },
    "shared-ui-components": {
      "root": "libs/shared/ui-components",
      "sourceRoot": "libs/shared/ui-components/src",
      "projectType": "ui-library",
      "tags": [],
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "libs/shared/ui-components/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "{workspaceRoot}/coverage/{projectRoot}"
          ],
          "options": {
            "jestConfig": "libs/shared/ui-components/jest.config.ts",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-util-common": {
      "root": "libs/shared/util-common",
      "sourceRoot": "libs/shared/util-common/src",
      "projectType": "library",
      "tags": [],
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "libs/shared/util-common/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "{workspaceRoot}/coverage/{projectRoot}"
          ],
          "options": {
            "jestConfig": "libs/shared/util-common/jest.config.ts",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
