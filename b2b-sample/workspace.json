{
  "version": 2,
  "projects": {
    "business-app": {
      "root": "apps/business-app",
      "type": "application"
    },
    "shared-assets": {
      "root": "libs/shared/assets",
      "sourceRoot": "libs/shared/assets/src",
      "projectType": "library",
      "tags": [],
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "libs/shared/assets/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "{workspaceRoot}/coverage/{projectRoot}"
          ],
          "options": {
            "jestConfig": "libs/shared/assets/jest.config.ts",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
